<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="tva.kastel.kit.ui.Controller">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#importFiles" text="Import.."/>
                        <MenuItem mnemonicParsing="false" text="Show files" onAction="#showFiles"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <SplitPane dividerPositions="0.29797979797979796" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane fx:id="anchorPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <ListView fx:id="listView" onMouseClicked="#listViewItemClicked" VBox.vgrow="ALWAYS"/>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Button fx:id="showTreeButton" alignment="CENTER" mnemonicParsing="false"
                                                onMouseClicked="#showTree" text="Show tree">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="12.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </Button>
                                        <Button fx:id="compareButton" alignment="CENTER" mnemonicParsing="false"
                                                onMouseClicked="#compare" text="Compare">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="12.0" right="5.0" top="5.0"/>
                                            </HBox.margin>
                                        </Button>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
                <AnchorPane>
                    <children>
                        <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE"
                                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                 AnchorPane.topAnchor="0.0">
                            <tabs>
                                <Tab fx:id="familyModelTab" text="Family Model">
                                    <content>
                                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <children>
                                                <TreeView fx:id="treeView"/>
                                                <TableView fx:id="tableView">
                                                    <columns>
                                                    </columns>
                                                </TableView>
                                            </children>
                                        </VBox>
                                    </content>
                                </Tab>
                                <Tab fx:id="taxonomyTab" text="Taxonomy">
                                    <content>
                                        <ScrollPane>
                                            <content>
                                                <AnchorPane>
                                                    <children>
                                                        <Canvas fx:id="canvas" AnchorPane.bottomAnchor="0.0"
                                                                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                                AnchorPane.topAnchor="0.0"/>
                                                    </children>
                                                </AnchorPane>
                                            </content>
                                        </ScrollPane>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</VBox>
